<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Version>0.1.6</Version>
    <Authors>Leon Spors</Authors>
    <Company>Spors</Company>
    <Copyright>Copyright (C) 2020 Spors</Copyright>
    <RepositoryUrl>https://github.com/LeonSpors/blazor-components</RepositoryUrl>
    <PackageTags>Blazor, Grids, Charts</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Description>Spors Blazor library is built to provide touch-friendly components. It includes two components, grid and chart.</Description>
    <PackageId>Spors.Blazor</PackageId>
    <Product>Spors.Blazor</Product>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
  </PropertyGroup>

  
  <ItemGroup>
    <SupportedPlatform Include="browser" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Humanizer.Core" Version="2.8.26" />
    <PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="5.0.0" />
  </ItemGroup>
</Project>
